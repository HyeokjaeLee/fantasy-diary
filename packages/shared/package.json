{
  "name": "@fantasy-diary/shared",
  "version": "0.1.0",
  "private": true,
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "bun x tsc --noEmit",
    "gen:supabase": "bun run ./scripts/generate-supabase-type.ts && bun run ./scripts/generate-supabase-zod.ts",
